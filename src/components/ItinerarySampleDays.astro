---
import SampleItineraryDay from '../components/SampleItineraryDay.astro';
const itinerary = Astro.props.itinerary;
---

{
    itinerary.dayItineraries &&
        itinerary.dayItineraries.map((dayItinerary: any) => (
            <SampleItineraryDay key={dayItinerary.id} itinerary={itinerary} dayItinerary={dayItinerary} />
        ))
}
